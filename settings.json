{
  "workingDirectory": "C:\\Users\\anzuy\\Projects\\AtCoder",
  "statusLine": {
    "enabled": true,
    "format": "{{branch}} | {{file}} | {{time}}"
  },
  "hooks": {
    "onFileEdit": [],
    "onBashCommand": [],
    "onToolUse": []
  },
  "ai": {
    "model": "claude-sonnet-4",
    "temperature": 0.1,
    "maxTokens": 4096
  },
  "editor": {
    "tabSize": 4,
    "insertSpaces": true,
    "wordWrap": true
  },
  "terminal": {
    "shell": "cmd.exe",
    "encoding": "utf-8"
  },
  "fileAssociations": {
    "*.cpp": "cpp",
    "*.cs": "csharp",
    "*.csx": "csharp",
    "*.md": "markdown"
  },
  "excludePatterns": [
    "**/.git/**",
    "**/node_modules/**",
    "**/bin/**",
    "**/obj/**",
    "**/.vs/**"
  ]
}